@using BugTracker.Models

@{
    ViewBag.Title = "My Tickets Page";
}

@{
var DispTicketsVM_Owner = (DispTicketsVM)ViewData["DispTicketsVM_Owner"];
}

@if (User.IsInRole("Developer"))
